{
  "name": "waproject-base",
  "version": "0.0.1",
  "description": "Api Base for WaProject",
  "main": "server/main.js",
  "readme": "README.md",
  "author": {
    "name": "Daniel Prado",
    "email": "danielprado.ad@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "waproject-repository"
  },
  "scripts": {
    "dev": "yarn ts-node-dev --inspect=0.0.0.0:5859 --respawn --transpileOnly --project tsconfig.dev.json ./src/server/main.ts",
    "lint": "yarn eslint \"./src/**/*.ts\"",
    "compile": "yarn compile:templates && yarn compile:server && yarn compile:migrations",
    "compile:migrations": "yarn tsc -p tsconfig.db.json",
    "compile:templates": "yarn copyfiles -f ./src/server/services/mail/templates/*.pug ./bin/server/services/mail/templates/",
    "compile:server": "yarn tsc",
    "watch:templates": "yarn watch \"yarn compile:templates\" ./src/server/services/mail/templates",
    "watch:server": "yarn tsc -w --preserveWatchOutput",
    "pretest": "yarn compile:migrations",
    "test": "cross-env NODE_PATH=./src/server jest --maxWorkers=1",
    "pretest:watch": "yarn compile:migrations",
    "test:watch": "cross-env NODE_PATH=./src/server jest --maxWorkers=1 --watch --bail",
    "pretest:cov": "yarn compile:migrations",
    "test:cov": "cross-env NODE_PATH=./src/server jest --maxWorkers=1 --coverage",
    "docker:release": "yarn docker:build && yarn docker:push",
    "docker:build": "docker build -t waproject/rankmyapp-api -f docker/prod/Dockerfile .",
    "docker:push": "docker push waproject/rankmyapp-api"
  },
  "husky": {
    "hooks": {
      "pre-commit": "concurrently -r \"yarn lint\" \"yarn tsc --noEmit\"",
      "post-merge": "yarn"
    }
  },
  "license": "ISC",
  "dependencies": {
    "@sentry/node": "5.6.2",
    "@vtex/phone": "4.8.3",
    "app-module-path": "2.2.0",
    "axios": "0.19.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "1.19.0",
    "connect-timeout": "1.9.0",
    "express": "4.17.3",
    "faker": "4.1.0",
    "fb": "2.0.0",
    "googleapis": "42.0.0",
    "inline-css": "2.4.2",
    "joi": "14.3.1",
    "jsonwebtoken": "8.5.1",
    "knex": "^0.19.5",
    "lodash": "4.17.13",
    "mailgun-js": "0.22.0",
    "morgan": "1.9.1",
    "objection": "1.6.9",
    "password-generator": "2.2.0",
    "pg": "7.12.1",
    "pug": "2.0.4",
    "source-map-support": "0.5.13",
    "tslib": "1.10.0",
    "uuid": "3.3.3"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/body-parser": "1.17.1",
    "@types/connect-timeout": "0.0.34",
    "@types/express": "4.17.1",
    "@types/faker": "4.1.5",
    "@types/fb": "0.0.23",
    "@types/inline-css": "0.0.32",
    "@types/jest": "24.0.18",
    "@types/joi": "14.3.3",
    "@types/jsonwebtoken": "8.3.3",
    "@types/knex": "0.16.1",
    "@types/lodash": "4.14.137",
    "@types/mailgun-js": "0.22.3",
    "@types/morgan": "1.7.37",
    "@types/node": "12.7.2",
    "@types/pug": "2.0.4",
    "@types/request": "2.48.2",
    "@types/uuid": "3.4.5",
    "@typescript-eslint/eslint-plugin": "2.0.0",
    "@typescript-eslint/parser": "2.0.0",
    "concurrently": "4.1.2",
    "copyfiles": "2.1.1",
    "cross-env": "5.2.0",
    "eslint": "6.2.1",
    "husky": "3.0.4",
    "inquirer": "6.5.1",
    "jest": "24.9.0",
    "jest-extended": "0.11.2",
    "ora": "3.4.0",
    "rimraf": "3.0.0",
    "sqlite3": "4.1.0",
    "ts-jest": "24.0.2",
    "ts-node-dev": "1.0.0-pre.39",
    "typescript": "3.5.3",
    "watch": "1.0.2"
  }
}
